<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="446.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.uaspbo2.TarikTunaiController">

   <children>

      <!-- Label Judul -->
      <Label layoutX="160.0" layoutY="50.0" style="-fx-font-size: 24px; -fx-font-weight: bold;" text="Tarik Tunai" />

      <!-- Input Nominal -->
      <TextField fx:id="tfNominal" layoutX="103.0" layoutY="114.0" prefWidth="240" promptText="Masukkan jumlah nominal" />

      <!-- Tombol Tarik -->
      <Button layoutX="167.0" layoutY="164.0" onAction="#tarikTunai" prefWidth="100" style="-fx-background-color: #2196F3; -fx-text-fill: white; -fx-font-weight: bold;" text="Tarik Tunai" />

      <!-- Tombol Kembali -->
      <Button layoutX="14.0" layoutY="353.0" onAction="#goBack" style="-fx-background-color: #9E9E9E; -fx-text-fill: white;" text="Kembali" />

   </children>
</AnchorPane>
